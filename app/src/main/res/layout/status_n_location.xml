<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <RelativeLayout
        android:id="@+id/status_cont"
        android:layout_width="match_parent"
        android:layout_height="448dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="16dp"
        android:background="#F9F9F9">

        <TextView
            android:id="@+id/status"
            android:layout_width="59dp"
            android:layout_height="24dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="0dp"
            android:layout_marginTop="0dp"
            android:fontFamily="@font/red_hat_display"
            android:gravity="center_horizontal|top"
            android:lineSpacingExtra="-2sp"
            android:text="@string/status_home"
            android:textColor="#06122B"
            android:textSize="20sp"
            android:textStyle="bold" />

        <RelativeLayout

            android:id="@+id/status_metr"
            android:layout_width="match_parent"
            android:layout_height="112dp"
            android:layout_below="@id/status"
            android:layout_alignParentStart="true"
            android:layout_alignParentEnd="true"
            android:layout_marginStart="0dp"
            android:layout_marginTop="12dp"
            android:layout_marginEnd="0dp"
            android:background="#F9F9F9">

            <RelativeLayout
                android:id="@+id/asset_sold"
                android:layout_width="110dp"
                android:layout_height="112dp"
                android:layout_alignParentStart="true"
                android:layout_marginStart="0dp"
                android:background="@drawable/some_id"
                android:elevation="8.97dp">

                <TextView
                    android:id="@+id/asset_sold_desc"
                    android:layout_width="45dp"
                    android:layout_height="40dp"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentTop="true"
                    android:layout_marginStart="12dp"
                    android:layout_marginTop="8dp"
                    android:gravity="top"
                    android:lineSpacingExtra="-1sp"
                    android:text="@string/asset_sold"
                    android:textColor="#818896"
                    android:textSize="16sp" />

                <TextView
                    android:id="@+id/no_asset_sold"
                    android:layout_width="24dp"
                    android:layout_height="30dp"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentBottom="true"
                    android:layout_marginStart="12dp"
                    android:layout_marginBottom="8dp"
                    android:fontFamily="@font/red_hat_display"
                    android:gravity="top"
                    android:lineSpacingExtra="-2sp"
                    android:text="10"
                    android:textColor="#06122B"
                    android:textSize="24sp"
                    android:textStyle="bold" />

            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/asset_in_stock"
                android:layout_width="110dp"
                android:layout_height="112dp"
                android:layout_alignParentTop="true"
                android:layout_alignParentBottom="true"
                android:layout_marginStart="22dp"
                android:layout_marginTop="0dp"
                android:layout_marginEnd="22dp"
                android:layout_marginBottom="0dp"
                android:layout_toStartOf="@+id/expired_asset"
                android:layout_toEndOf="@+id/asset_sold"
                android:background="@drawable/some_id"
                android:elevation="8.97dp"
                android:gravity="center">

                <TextView
                    android:id="@+id/asset_in_stock_desc"
                    android:layout_width="58dp"
                    android:layout_height="40dp"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentTop="true"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentBottom="true"
                    android:layout_marginStart="12dp"
                    android:layout_marginTop="8dp"
                    android:layout_marginEnd="33dp"
                    android:layout_marginBottom="64dp"
                    android:gravity="top"
                    android:lineSpacingExtra="-1sp"
                    android:text="@string/asset_in_stock"
                    android:textColor="#818896"
                    android:textSize="16sp" />

                <TextView
                    android:id="@+id/no_asset_in_stock"
                    android:layout_width="15dp"
                    android:layout_height="30dp"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentBottom="true"
                    android:layout_marginStart="12dp"
                    android:layout_marginBottom="8dp"
                    android:fontFamily="@font/red_hat_display"
                    android:gravity="top"
                    android:lineSpacingExtra="-2sp"
                    android:text="3"
                    android:textColor="#06122B"
                    android:textSize="24sp"
                    android:textStyle="bold" />

            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/expired_asset"
                android:layout_width="110dp"
                android:layout_height="112dp"
                android:layout_alignParentEnd="true"
                android:layout_marginEnd="0dp"
                android:background="@drawable/some_id"
                android:elevation="8.97dp">

                <TextView
                    android:id="@+id/expired_ass"
                    android:layout_width="56dp"
                    android:layout_height="40dp"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentTop="true"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentBottom="true"
                    android:layout_marginStart="12dp"
                    android:layout_marginTop="8dp"
                    android:layout_marginEnd="35dp"
                    android:layout_marginBottom="64dp"
                    android:gravity="top"
                    android:lineSpacingExtra="-1sp"
                    android:text="@string/expired_asset"
                    android:textColor="#818896"
                    android:textSize="16sp" />

                <TextView
                    android:id="@+id/no_expired_asset"
                    android:layout_width="14dp"
                    android:layout_height="30dp"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentBottom="true"
                    android:layout_marginStart="12dp"
                    android:layout_marginBottom="8dp"
                    android:fontFamily="@font/red_hat_display"
                    android:gravity="top"
                    android:lineSpacingExtra="-2sp"
                    android:text="5"
                    android:textColor="#06122B"
                    android:textSize="24sp"
                    android:textStyle="bold" />

            </RelativeLayout>


        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/chart_status"
            android:layout_width="match_parent"
            android:layout_height="288dp"
            android:layout_below="@+id/status_metr"
            android:layout_marginTop="12dp"
            android:background="@drawable/chart_status"
            android:clipToOutline="true"
            android:elevation="6.16dp">

            <TextView
                android:id="@+id/chart_status_title"
                android:layout_width="42dp"
                android:layout_height="20dp"
                android:layout_alignParentLeft="true"
                android:layout_alignParentTop="true"
                android:layout_marginLeft="12dp"
                android:layout_marginTop="8dp"
                android:fontFamily="@font/red_hat_display"
                android:gravity="top"
                android:lineSpacingExtra="-1sp"
                android:text="@string/chart_home"
                android:textColor="#06122B"
                android:textSize="16sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/some_id_chart_status"
                android:layout_width="11dp"
                android:layout_height="200dp"
                android:layout_alignParentLeft="true"
                android:layout_alignParentTop="true"
                android:layout_marginLeft="13.91dp"
                android:layout_marginTop="45.81dp"
                android:fontFamily="@font/inter"
                android:gravity="right|top"
                android:text="@string/some_id_chart_status"
                android:textColor="#818896"
                android:textSize="10sp" />

            <RelativeLayout
                android:id="@+id/basic_bar"
                android:layout_width="265dp"
                android:layout_height="191dp"
                android:layout_alignParentLeft="true"
                android:layout_alignParentTop="true"
                android:layout_alignParentRight="true"
                android:layout_marginLeft="32dp"
                android:layout_marginTop="52dp"
                android:layout_marginRight="32dp">

                <View
                    android:id="@+id/bar_1"
                    android:layout_width="25dp"
                    android:layout_height="143dp"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentBottom="true"
                    android:layout_marginStart="87dp"
                    android:background="#00B6AC" />

                <View
                    android:id="@+id/bar_2"
                    android:layout_width="25dp"
                    android:layout_height="31dp"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentBottom="true"
                    android:layout_marginStart="145dp"
                    android:layout_marginEnd="145dp"
                    android:background="#FF7C45" />

                <View
                    android:id="@+id/bar_3"
                    android:layout_width="25dp"
                    android:layout_height="75dp"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentBottom="true"
                    android:layout_marginEnd="87dp"
                    android:background="#FF6169" />

            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/legend"
                android:layout_width="167.6dp"
                android:layout_height="15dp"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_alignParentEnd="true"
                android:layout_marginStart="108dp"
                android:layout_marginTop="265dp"
                android:layout_marginEnd="103dp">

                <RelativeLayout
                    android:id="@+id/s_1"
                    android:layout_width="36.53dp"
                    android:layout_height="15dp">

                    <View
                        android:id="@+id/ellipse_green"
                        android:layout_width="8.53dp"
                        android:layout_height="8.53dp"
                        android:layout_alignParentStart="true"
                        android:layout_alignParentTop="true"
                        android:layout_alignParentBottom="true"
                        android:layout_marginTop="3.23dp"
                        android:layout_marginBottom="3.23dp"
                        android:background="#00B6AC" />

                    <TextView
                        android:id="@+id/sold"
                        android:layout_width="23dp"
                        android:layout_height="15dp"
                        android:layout_alignParentEnd="true"
                        android:fontFamily="@font/red_hat_display"
                        android:gravity="top"
                        android:text="@string/sold_chart"
                        android:textColor="#06122B"
                        android:textSize="11sp"
                        android:textStyle="bold" />

                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/s_2"
                    android:layout_width="42.53dp"
                    android:layout_height="15dp"
                    android:layout_marginStart="18dp"
                    android:layout_toEndOf="@+id/s_1">

                    <View
                        android:id="@+id/ellipse_orange_s"
                        android:layout_width="8.53dp"
                        android:layout_height="8.53dp"
                        android:layout_alignParentStart="true"
                        android:layout_alignParentTop="true"
                        android:layout_alignParentBottom="true"
                        android:layout_marginTop="3.23dp"
                        android:layout_marginBottom="3.23dp"
                        android:background="#FF7C45" />

                    <TextView
                        android:id="@+id/stock_s"
                        android:layout_width="29dp"
                        android:layout_height="15dp"
                        android:layout_alignParentEnd="true"
                        android:fontFamily="@font/red_hat_display"
                        android:gravity="top"
                        android:text="@string/stock_chart"
                        android:textColor="#06122B"
                        android:textSize="11sp"
                        android:textStyle="bold" />

                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/s_3"
                    android:layout_width="52.53dp"
                    android:layout_height="15dp"
                    android:layout_marginStart="18dp"
                    android:layout_toEndOf="@+id/s_2">

                    <View
                        android:id="@+id/ellipse_pink"
                        android:layout_width="8.53dp"
                        android:layout_height="8.53dp"
                        android:layout_alignParentStart="true"
                        android:layout_alignParentTop="true"
                        android:layout_alignParentBottom="true"
                        android:layout_marginTop="3.23dp"
                        android:layout_marginBottom="3.23dp"
                        android:background="#FF6169" />

                    <TextView
                        android:id="@+id/expired"
                        android:layout_width="39dp"
                        android:layout_height="15dp"
                        android:layout_alignParentEnd="true"
                        android:fontFamily="@font/red_hat_display"
                        android:gravity="top"
                        android:text="@string/expired_chart"
                        android:textColor="#06122B"
                        android:textSize="11sp"
                        android:textStyle="bold" />

                </RelativeLayout>

            </RelativeLayout>

        </RelativeLayout>

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/lokasi_cont"
        android:layout_width="match_parent"
        android:layout_height="419dp"
        android:layout_below="@+id/status_cont"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="16dp">

        <TextView
            android:id="@+id/location"
            android:layout_width="81dp"
            android:layout_height="24dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="0dp"
            android:layout_marginTop="0dp"
            android:fontFamily="@font/red_hat_display"
            android:gravity="center_horizontal|top"
            android:lineSpacingExtra="-2sp"
            android:text="@string/location_home"
            android:textColor="#06122B"
            android:textSize="20sp"
            android:textStyle="bold" />

        <RelativeLayout
            android:id="@+id/some_id_gudang"
            android:layout_width="185dp"
            android:layout_height="83dp"
            android:layout_below="@+id/location"
            android:layout_alignParentStart="true"
            android:layout_marginStart="0dp"
            android:layout_marginTop="12dp"
            android:background="@drawable/some_id"
            android:elevation="8.97dp">

            <TextView
                android:id="@+id/gudang"
                android:layout_width="59dp"
                android:layout_height="20dp"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_marginStart="12dp"
                android:layout_marginTop="8dp"
                android:fontFamily="@font/red_hat_display"
                android:gravity="top"
                android:lineSpacingExtra="-1sp"
                android:text="@string/gudang_location"
                android:textColor="#818896"
                android:textSize="16sp" />

            <TextView
                android:id="@+id/no_gudang"
                android:layout_width="15dp"
                android:layout_height="30dp"
                android:layout_alignParentStart="true"
                android:layout_alignParentBottom="true"
                android:layout_marginStart="12dp"
                android:layout_marginBottom="8dp"
                android:fontFamily="@font/red_hat_display"
                android:gravity="top"
                android:lineSpacingExtra="-2sp"
                android:text="8"
                android:textColor="#06122B"
                android:textSize="24sp"
                android:textStyle="bold" />

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/some_id_rak_penjualan"
            android:layout_width="185dp"
            android:layout_height="83dp"
            android:layout_below="@+id/location"
            android:layout_alignParentEnd="true"
            android:layout_marginTop="12dp"
            android:background="@drawable/some_id"
            android:elevation="8.97dp">

            <TextView
                android:id="@+id/rak_penjual"
                android:layout_width="101dp"
                android:layout_height="20dp"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_marginStart="12dp"
                android:layout_marginTop="8dp"
                android:fontFamily="@font/red_hat_display"
                android:gravity="top"
                android:lineSpacingExtra="-1sp"
                android:text="@string/rak_location"
                android:textColor="#818896"
                android:textSize="16sp" />

            <TextView
                android:id="@+id/no_rak"
                android:layout_width="24dp"
                android:layout_height="30dp"
                android:layout_alignParentStart="true"
                android:layout_alignParentBottom="true"
                android:layout_marginStart="12dp"
                android:layout_marginBottom="8dp"
                android:fontFamily="@font/red_hat_display"
                android:gravity="top"
                android:lineSpacingExtra="-2sp"
                android:text="10"
                android:textColor="#06122B"
                android:textSize="24sp"
                android:textStyle="bold" />

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/chart_location"
            android:layout_width="match_parent"
            android:layout_height="288dp"
            android:layout_below="@+id/some_id_gudang"
            android:layout_marginTop="12dp"
            android:background="@drawable/chart_location"
            android:clipToOutline="true"
            android:elevation="6.16dp">

            <TextView
                android:id="@+id/chart_location_title"
                android:layout_width="42dp"
                android:layout_height="20dp"
                android:layout_alignParentLeft="true"
                android:layout_alignParentTop="true"
                android:layout_marginLeft="12dp"
                android:layout_marginTop="8dp"
                android:fontFamily="@font/red_hat_display"
                android:gravity="top"
                android:lineSpacingExtra="-1sp"
                android:text="@string/chart_home"
                android:textColor="#06122B"
                android:textSize="16sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/some_id_chart_location"
                android:layout_width="12dp"
                android:layout_height="200dp"
                android:layout_alignParentLeft="true"
                android:layout_alignParentTop="true"
                android:layout_marginLeft="13.91dp"
                android:layout_marginTop="45.81dp"
                android:fontFamily="@font/inter"
                android:gravity="right|top"
                android:text="@string/some_id_chart_location"
                android:textColor="#818896"
                android:textSize="10sp" />

            <RelativeLayout
                android:id="@+id/basic_bar_location"
                android:layout_width="265dp"
                android:layout_height="191dp"
                android:layout_alignParentLeft="true"
                android:layout_alignParentTop="true"
                android:layout_alignParentRight="true"
                android:layout_marginLeft="32dp"
                android:layout_marginTop="52dp"
                android:layout_marginRight="32dp">

                <View
                    android:id="@+id/bar_loc_1"
                    android:layout_width="25dp"
                    android:layout_height="78dp"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentBottom="true"
                    android:layout_marginStart="116dp"
                    android:background="#00B6AC" />

                <View
                    android:id="@+id/bar_loc_2"
                    android:layout_width="25dp"
                    android:layout_height="95dp"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentBottom="true"
                    android:layout_marginStart="174dp"
                    android:layout_marginEnd="116dp"
                    android:background="#FF7C45" />

            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/legend_loc"
                android:layout_width="156.06dp"
                android:layout_height="15dp"
                android:layout_alignParentTop="true"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="265dp">

                <RelativeLayout
                    android:id="@+id/l_1"
                    android:layout_width="54.53dp"
                    android:layout_height="15dp">

                    <View
                        android:id="@+id/ellipse_green_loc"
                        android:layout_width="8.53dp"
                        android:layout_height="8.53dp"
                        android:layout_alignParentStart="true"
                        android:layout_alignParentTop="true"
                        android:layout_alignParentBottom="true"
                        android:layout_marginTop="3.23dp"
                        android:layout_marginBottom="3.23dp"
                        android:background="#00B6AC" />

                    <TextView
                        android:id="@+id/gudang_l"
                        android:layout_width="41dp"
                        android:layout_height="15dp"
                        android:layout_alignParentEnd="true"
                        android:layout_marginStart="5dp"
                        android:layout_toEndOf="@+id/ellipse_green_loc"
                        android:fontFamily="@font/red_hat_display"
                        android:gravity="top"
                        android:text="@string/gudang_location"
                        android:textColor="#06122B"
                        android:textSize="11sp"
                        android:textStyle="bold" />

                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/l_2"
                    android:layout_width="83.53dp"
                    android:layout_height="15dp"
                    android:layout_alignParentEnd="true"
                    android:layout_marginStart="18dp"
                    android:layout_marginEnd="0dp"
                    android:layout_toEndOf="@+id/l_1">

                    <View
                        android:id="@+id/ellipse_orange"
                        android:layout_width="8.53dp"
                        android:layout_height="8.53dp"
                        android:layout_alignParentStart="true"
                        android:layout_alignParentTop="true"
                        android:layout_alignParentBottom="true"
                        android:layout_marginTop="3.23dp"
                        android:layout_marginBottom="3.23dp"
                        android:background="#FF7C45" />

                    <TextView
                        android:id="@+id/stock"
                        android:layout_width="29dp"
                        android:layout_height="15dp"
                        android:layout_alignParentEnd="true"
                        android:layout_marginStart="5dp"
                        android:layout_toEndOf="@+id/ellipse_orange"
                        android:fontFamily="@font/red_hat_display"
                        android:gravity="top"
                        android:text="@string/rak_location"
                        android:textColor="#06122B"
                        android:textSize="11sp"
                        android:textStyle="bold" />

                </RelativeLayout>

            </RelativeLayout>

        </RelativeLayout>

    </RelativeLayout>

</RelativeLayout>